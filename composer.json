{
    "name": "converallel/oauth-provider",
    "description": "OAuth 2 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Xun Chen",
            "role": "Maintainer"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authorisation"
    ],
    "require": {
        "php": ">=5.6.0",
        "league/oauth2-client": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "psr-4": {
            "Converallel\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Converallel\\OAuth2\\Client\\Test\\": "tests/"
        }
    }
}
